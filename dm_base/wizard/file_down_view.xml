<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="wizard_file_down_view" model="ir.ui.view">
            <field name="name">Download File</field>
            <field name="model">file.down</field>
            <field name="arch" type="xml">
                <form string="Download File" version="7.0">
                    <field name="filename" invisible="1"/>
                    <field name="filedata" filename="filename" readonly="1"/>
                </form>
            </field>
        </record>
        <record id="action_wizard_file_down" model="ir.actions.act_window">
            <field name="name">Download File</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">file.down</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
    </data>
</openerp>
