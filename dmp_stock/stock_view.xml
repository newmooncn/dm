<!-- material request line -->
		<record id="view_mat_line_tree" model="ir.ui.view">
		    <field name="name">req.line.req.tree</field>
		    <field name="model">material.request.line</field>
		    <field name="arch" type="xml">	
                <tree string="Stock Moves" editable="bottom">
	                <field name="name" invisible="1"/>
                    <field name="product_id" on_change="onchange_product_id(product_id,location_id,location_dest_id)"/>
                    <field name="product_qty"/>
                    <field name="product_uom" readonly="1" string="Unit of Measure" groups="product.group_uom"/>
                    <field name="product_uos_qty"  invisible="1"/>
                    <field name="product_uos"  invisible="1"/>     
					<field name="price_unit" groups="account.group_account_invoice"/> 
					<field name="price_subtotal"/>
					<field name="price_currency_id" invisible="1"/>	                                       
		            <field name="mr_emp_id"/>
		            <field name="mr_sale_prod_id"/>
		            <field name="mr_notes"/>
                    <field name="location_id" groups="stock.group_locations"/>
                    <field name="location_dest_id" groups="stock.group_locations"/>
                    <field name="create_uid" invisible="1"/>
                    <field name="create_date" invisible="1"/>
                    <field name="state"/>        
               </tree>
		    </field>
		</record>
                		
        <record id="view_mat_line_form" model="ir.ui.view">
            <field name="name">req.line.req.form</field>
            <field name="model">material.request.line</field>
            <field name="arch" type="xml">
	            <form string="Stock Moves" version="7.0">
	                <header>
	                        <button name="force_assign" states="confirmed" string="Force Availability" type="object" groups="base.group_user"/>
	                        <button name="action_confirm" states="draft" string="Confirm" type="object" groups="base.group_user"/>
	                        <button name="cancel_assign" states="assigned" string="Cancel Availability" type="object" groups="base.group_user"/>
	                        <field name="state" widget="statusbar" statusbar_visible="draft,assigned,done"/>
	                </header>
					<group>
	                        <field name="name" invisible="1"/>
	                        <field name="product_id" on_change="onchange_product_id(product_id,location_id,location_dest_id)"/>
					</group>
                    <group>
	                    <group>
	                        <field name="type" invisible="1"/>
	                        <label for="product_qty"/>
	                        <div>
	                            <field name="product_qty" on_change="onchange_quantity(product_id, product_qty, product_uom, product_uos)" class="oe_inline"/>
	                            <field name="product_uom" readonly="1" string="Unit Of Measure" groups="product.group_uom" class="oe_inline"/>
	                        </div>
	                        <field name="product_uos_qty" invisible="1" on_change="onchange_uos_quantity(product_id, product_uos_qty, product_uos, product_uom)" class="oe_inline"/>
	                        <field name="product_uos" invisible="1" on_change="onchange_quantity(product_id, product_qty, product_uom, product_uos)" class="oe_inline"/>
							<field name="price_unit" groups="account.group_account_invoice"/>
							<field name="price_currency_id" invisible="1"/>
							<field name="mr_emp_id"/>
							<field name="mr_sale_prod_id"/>                       
	                    </group>
						<group>
							<field name="location_id" domain="[('usage','&lt;&gt;','view')]"/>
							<field name="location_dest_id" domain="[('usage','&lt;&gt;','view')]"/>                       
							<field name="create_date"  readonly="1"/>
						</group>
					</group>
					<group>
							<field name="mr_notes"/>
					</group>
				</form>
            </field>
        </record>
        
        <record id="view_material_request_line_tree" model="ir.ui.view">
            <field name="name">req.line.tree</field>
            <field name="model">material.request.line</field>
            <field name="arch" type="xml">
                <tree colors="grey:state == 'cancel';green:state == 'done'" string="Moves">
                    <field name="picking_id"/>
                    <field name="mr_date_order"/>
                    <field name="mr_dept_id"/>
                    <field name="product_id" on_change="onchange_product_id(product_id,location_id,location_dest_id, False)"/>
                    <field name="product_qty" on_change="onchange_quantity(product_id, product_qty, product_uom, product_uos)"/>
                    <field name="product_uom" string="Unit of Measure" groups="product.group_uom"/>
					<field name="price_unit" groups="account.group_account_invoice"/> 
					<field name="price_subtotal"/>
					<field name="price_currency_id" invisible="1"/>	 
                    <field name="mr_emp_id"/>
                    <field name="mr_sale_prod_id"/>
                    <field name="mr_notes"/>
                    <field name="name"/>
                    <field name="location_id" groups="stock.group_locations"/>
                    <field name="location_dest_id" groups="stock.group_locations"/>
                    <field name="create_date"/>
                    <field name="state"/>             
                </tree>
            </field>
        </record>
                        
        <record id="view_material_request_line_form" model="ir.ui.view">
            <field name="name">req.line.form</field>
            <field name="model">material.request.line</field>
            <field name="inherit_id" ref="stock.view_move_form"/>
            <field name="arch" type="xml">
				<xpath expr="/form/sheet" position="replace">	                
					<group>
	                        <field name="name" invisible="1"/>
	                        <field name="product_id" on_change="onchange_product_id(product_id,location_id,location_dest_id)"/>
					</group>
                    <group>
	                    <group>
	                        <field name="type" invisible="1"/>
	                        <label for="product_qty"/>
	                        <div>
	                            <field name="product_qty" on_change="onchange_quantity(product_id, product_qty, product_uom, product_uos)" class="oe_inline"/>
	                            <field name="product_uom" string="Unit Of Measure" groups="product.group_uom" class="oe_inline"/>
	                        </div>
	                        <field name="product_uos_qty" invisible="1" on_change="onchange_uos_quantity(product_id, product_uos_qty, product_uos, product_uom)" class="oe_inline"/>
	                        <field name="product_uos" invisible="1" on_change="onchange_quantity(product_id, product_qty, product_uom, product_uos)" class="oe_inline"/>
							<field name="price_unit" groups="account.group_account_invoice"/>
							<field name="price_currency_id" invisible="1"/>	                        
							<field name="mr_emp_id"/>
							<field name="mr_sale_prod_id"/>                       
	                    </group>
						<group>
							<field name="location_id" domain="[('usage','&lt;&gt;','view')]"/>
							<field name="location_dest_id" domain="[('usage','&lt;&gt;','view')]"/>                       
							<field name="create_date"  readonly="1"/>
						</group>
					</group>
                    <group>
	                    <group>
		                    <field name="mr_dept_id" readonly="1"/>                   
	                    </group>
						<group>
		                    <field name="mr_date_order" readonly="1"/>
						</group>
					</group>
					<group>
							<field name="mr_notes"/>
					</group>
				</xpath>
            </field>
        </record>   
        <record id="view_material_request_line_search" model="ir.ui.view">
            <field name="name">material.request.line.search</field>
            <field name="model">material.request.line</field>
            <field name="arch" type="xml">
                <search string="Material Req/Ret Products">
                    <field name="name"/>
                    <field name="date" string="Order Date"/>
					<filter icon="terp-stock" string="Request" name="data_mr" domain="[('type','=','mr')]"/>
					<filter icon="terp-stock" string="Return" name="data_mr" domain="[('type','=','mrr')]"/>
                    <filter icon="terp-camera_test" string="Ready" name="ready" domain="[('state','=','assigned')]" help="Stock moves that are Available (Ready to process)"/>
                    <filter icon="terp-stock" string="To Do" name="future" domain="[('state','in',('assigned','confirmed','waiting'))]" help="Stock moves that are Confirmed, Available or Waiting"/>
                    <filter icon="terp-dialog-close" string="Done" name="done" domain="[('state','=','done')]" help="Stock moves that have been processed"/>
                    <separator/>
                    <filter icon="terp-go-today" string="Today" domain="[('date','&lt;=',time.strftime('%%Y-%%m-%%d 23:59:59')),('date','&gt;=',time.strftime('%%Y-%%m-%%d 00:00:00'))]" help="Orders processed Today or planned for Today"/>
                    
                    <!-- 
                    Change the prodcut_id to allow to remove the default active=True restriction                    
                     -->
                    <!-- <field name="product_id"/> -->
                    <field name="product_id" domain="['|',('active','=',True),('active','=',False)]"/>
                    <field name="prod_categ_id"/>
                    <field name="mr_dept_id"/>
                    <field name="mr_emp_id"/>
                    <field name="mr_sale_prod_id"/>
                    <group expand="0" string="Group By...">
                    	<filter string="ID" name="by_id" icon="terp-accessories-archiver" domain="[]"  context="{'group_by':'mr_sale_prod_id'}"/>
                        <filter string="Department" name="by_dept" icon="terp-accessories-archiver" domain="[]"  context="{'group_by':'mr_dept_id'}"/>
                        <filter string="Employee" name="by_emp" icon="terp-accessories-archiver" domain="[]"  context="{'group_by':'mr_emp_id'}"/>                        
                        <filter string="Product" name="by_product" icon="terp-accessories-archiver" domain="[]"  context="{'group_by':'product_id'}"/>
                        <!-- <filter string="Product Category" name="by_product_categ" icon="terp-accessories-archiver" domain="[]"  context="{'group_by':'prod_categ_id'}"/> -->
                        <filter string="Material Request" name="groupby_picking_id" icon="terp-accessories-archiver" domain="[]"  context="{'group_by':'picking_id'}"/>
                        <filter icon="terp-stock_effects-object-colorize" string="Status" domain="[]" context="{'group_by':'state'}"/>
                        <filter string="Date" name="groupby_date" icon="terp-go-month" domain="[]" context="{'group_by':'mr_date_order'}"/>
                    </group>
                </search>
            </field>
        </record>
        <record id="action_material_request_line" model="ir.actions.act_window">
            <field name="name">Material Req/Ret Products</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">material.request.line</field>
            <field name="view_type">form</field>
        	<field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_material_request_line_tree"/>
            <field name="search_view_id" ref="view_material_request_line_search"/>
            <field name="domain">['|',('picking_id.type','=','mr'),('picking_id.type','=','mrr')]</field>
        </record>
        
        <record model="ir.actions.act_window.view" id="action_material_request_line_tree">
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_material_request_line_tree"/>
            <field name="act_window_id" ref="action_material_request_line"/>
        </record>

        <record model="ir.actions.act_window.view" id="action_material_request_line_form">
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_material_request_line_form"/>
        	<field name="act_window_id" ref="action_material_request_line"/>
        </record>
        <menuitem action="action_material_request_line" id="menu_action_material_request_line" 
        	parent="stock.menu_stock_products_moves" sequence="3" groups="stock.group_stock_user"/>
<!-- add the order date filter -->      
        <record id="view_picking_internal_search" model="ir.ui.view">
            <field name="name">dmp.stock.picking.search</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_internal_search"/>
