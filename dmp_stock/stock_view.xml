<!-- add the messages to internal move -->	
        <record id="dmp_view_picking_form" model="ir.ui.view">
            <field name="name">dmp.stock.picking.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>	
            <field name="arch" type="xml">
	            <xpath expr="/form/sheet" position="after">
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </xpath> 
 				<notebook position="inside">
 					<page string="Account Moves" groups="account.group_account_user">
 						<field name='account_move_ids' attrs="{'readonly': [('state','!=','done')]}"/>
 					</page>
 				</notebook>
            	<!-- Only warehouse user can do stcoking actions -->
	            <xpath expr="/form/header" position="attributes">
                	<attribute name="groups">stock.group_stock_user</attribute>
                </xpath>
                <!-- Remove the 'Confirm and Transfter' button for the forcing assign under 'draft' state, and replace with the checking available button -->
                <xpath expr="//button[@name='draft_validate']" position="replace">
                    <button name="action_assign" states="confirmed" string="Check Availability" type="object" class="oe_highlight"/>
                </xpath>
                <!-- Remove the 'Force Assign' button when there are not available inventory under 'confirmed' state -->
                <xpath expr="//button[@name='force_assign']" position="replace">
                </xpath>                    
        	</field>        
		</record>
<!-- adjust  the stock in -->	
        <record id="dmp_view_picking_in_form" model="ir.ui.view">
            <field name="name">dmp.stock.picking.in.form</field>
            <field name="model">stock.picking.in</field>
            <field name="inherit_id" ref="stock.view_picking_in_form"/>	
            <field name="arch" type="xml">
            	<!-- Only warehouse user can do stcoking actions -->
	            <xpath expr="/form/header" position="attributes">
                	<attribute name="groups">stock.group_stock_user</attribute>
                </xpath>    
            	<field name="date" position="attributes">
            		<attribute name="widget">date</attribute>>
            	</field>
            	<field name="date" position="before">
            		<field name="create_date"/>
            	</field> 
            	<field name="date_done" position="replace">
            	</field>            	
            	<field name="origin" position="replace">
            		<field name="date_done" string="Date of Reception"/>
            		<field name="deliver_ticket_no" attrs="{'readonly':[('state','!=','done')]}"/>
            	</field>          
            	<field name="stock_journal_id" position="after">
            		<field name="origin" placeholder="e.g. PO0032" class="oe_inline"/>
            	</field>          
            	
            	<field name="min_date" position="attributes">
            		<attribute name="widget">date</attribute>
            	</field>
                <xpath expr="//field[@name='move_lines']" position="replace">
                    <field name="move_lines" context="{'address_in_id': partner_id, 'picking_type': 'in', 'form_view_ref':'stock.view_move_picking_form', 'tree_view_ref':'stock.view_move_picking_tree'}"/>
                </xpath>
 				<notebook position="inside">
 					<page string="Account Moves" groups="account.group_account_user">
 						<field name='account_move_ids' attrs="{'readonly': [('state','!=','done')]}"/>
 					</page>
 				</notebook>       
 				<!-- change price and create invoice wizard
 				 
 				-->
                <xpath expr="//button[@string='Create Invoice/Refund']" position="attributes">
                	<attribute name="name">%(action_stock_invoice_onshipping_single)d</attribute>
                	<attribute name="groups">account.group_account_user</attribute>
                </xpath> 				                             	
        	</field>        
		</record>	
<!-- adjust the stock out -->		
        <record id="dmp_view_picking_out_form" model="ir.ui.view">
            <field name="name">dmp.stock.picking.out.form</field>
            <field name="model">stock.picking.out</field>
            <field name="inherit_id" ref="stock.view_picking_out_form"/>	
            <field name="arch" type="xml">
            	<!-- Only warehouse user can do stcoking actions -->
	            <xpath expr="/form/header" position="attributes">
                	<attribute name="groups">stock.group_stock_user</attribute>
                </xpath>            
 				<notebook position="inside">
 					<page string="Account Moves" groups="account.group_account_user">
 						<field name='account_move_ids' attrs="{'readonly': [('state','!=','done')]}"/>
 					</page>
 				</notebook>           
                <xpath expr="//button[@string='Create Invoice/Refund']" position="attributes">
                	<attribute name="name">%(action_stock_invoice_onshipping_single)d</attribute>
                	<attribute name="groups">account.group_account_user</attribute>
                </xpath> 	
                <!-- Remove the 'Confirm and Transfter' button for the forcing assign under 'draft' state -->
                <xpath expr="//button[@name='draft_validate']" position="replace">
                </xpath>
                <!-- Remove the 'Force Assign' button when there are not available inventory under 'confirmed' state -->
                <xpath expr="//button[@name='force_assign']" position="replace">
                </xpath>                			                        	
        	</field>        
		</record>		
<!-- add the supplier product name -->		
        <record id="dmp_view_move_picking_tree" model="ir.ui.view">
            <field name="name">dmp.stock.move.tree</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_picking_tree"/>	
            <field name="arch" type="xml">
            	<field name="product_qty" position="after">
            		<field name="return_qty"/>
            		<field name="price_unit" groups="account.group_account_invoice"/>
            	</field>
            	<field name="product_uom" position="after">
            		<field name="product_uom_base_qty"/>
            		<field name="product_uom_base"/>
            	</field>
            	<field name="state" position="before">
            		<field name="date"/>
            	</field>
        	</field>        
		</record>
				
        <record id="dmp_view_move_picking_form" model="ir.ui.view">
            <field name="name">dmp.stock.move.form</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_picking_form"/>	
            <field name="arch" type="xml">
				<field name="create_date" position="attributes">
					<attribute name="invisible">0</attribute>
				</field>
            	<field name="product_qty" position="after">
            		<field name="return_qty"/>
            		<field name="price_unit" groups="account.group_account_invoice"/>
            	</field>
            	<field name="product_uom" position="after">
            		<field name="product_uom_base_qty"/>
            		<field name="product_uom_base"/>
            	</field>
            	<field name="date_expected" position="after">
            		<field name="purchase_line_id"/>
            	</field>
            	<field name="date_expected" position="attributes">
            		<attribute name="widget">date</attribute>
            	</field>
            	<field name="partner_id" position="attributes">
					<attribute name="string">Supplier</attribute>
				</field>
            </field>
        </record>		
        <!--  for the incoming and deliver products -->
        <record id="dmp_view_move_tree_reception_picking" model="ir.ui.view">
            <field name="name">dmp.stock.move.tree2</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_tree_reception_picking"/>	
            <field name="arch" type="xml">
            	<field name="product_qty" position="after">
            		<field name="return_qty"/>
            		<field name="price_unit" groups="account.group_account_invoice"/>
            	</field>
            	<field name="product_uom" position="after">
            		<field name="product_uom_base_qty"/>
            		<field name="product_uom_base"/>
            	</field>
            	<field name="picking_id" position="attributes">
            		<attribute name="invisible">0</attribute>
				</field>
            </field>
        </record>		
                
        <record id="dmp_view_move_tree" model="ir.ui.view">
            <field name="name">dmp.stock.move.tree</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_tree"/>	
            <field name="arch" type="xml">
            	<field name="location_dest_id" position="after">
            		<field name="create_date"/>
            	</field>
            	<field name="date" position="replace">
            		<field name="date"/>
            	</field>
            	<field name="date_expected" position="attributes">
            		<attribute name="widget">date</attribute>
            	</field>
            	<field name="product_qty" position="after">
            		<field name="return_qty"/>
            		<field name="price_unit" groups="account.group_account_invoice"/>
            	</field>
            	<field name="product_uom" position="after">
            		<field name="product_uom_base_qty"/>
            		<field name="product_uom_base"/>
            	</field>
            	<field name="origin" position="after">
            		<field name="date"/>
            	</field>
            </field>
        </record>                 
                
        <record id="dmp_view_move_form" model="ir.ui.view">
            <field name="name">dmp.stock.move.form</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_form"/>	
            <field name="arch" type="xml">
            	<field name="product_qty" position="after">
            		<field name="return_qty"/>
            		<field name="price_unit" groups="account.group_account_invoice"/>
            	</field>
            	<field name="product_uom" position="after">
            		<field name="product_uom_base_qty"/>
            		<field name="product_uom_base"/>
            	</field>
            	<field name="date" position="replace">
            		<field name="date"/>
            	</field>
            </field>
        </record> 
        
        <record id="dmp_view_move_search" model="ir.ui.view">
            <field name="name">dmp.stock.move.search</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_search"/>	
            <field name="arch" type="xml">
            	<field name="product_id" position="replace">
                    <!-- 
                    Change the prodcut_id to allow to remove the default active=True restriction                    
                     -->
                    <field name="product_id" domain="['|',('active','=',True),('active','=',False)]"/>
            	</field>
            	<field name="product_id" position="after">
            		<field name="origin"/>
            	</field>
            	<!-- remove the  groups="base.group_no_one" -->
            	<field name="date" position='replace'>
            		 <field name="date"/>
            		 <field name="type"/>
            	</field>
            	<filter string="Today"  position='after'>
            		 <separator/>
            		 <filter string="Getting Goods" name="move_incoming" domain="[('type','=','in')]"/>
            		 <filter string="Sending Goods" name="move_incoming" domain="[('type','=','out')]"/>
            		 <filter string="Internal" name="move_incoming" domain="[('type','=','internal')]"/>
            		 <filter string="Material Request" name="move_incoming" domain="[('type','=','mr')]"/>
            		 <filter string="Material Return" name="move_incoming" domain="[('type','=','mrr')]"/>
            	</filter>
            </field>
        </record> 
        
        <record id="view_category_property_form_ext" model="ir.ui.view">
            <field name="name">view_category_property_form_ext</field>
            <field name="model">product.category</field>
            <field name="inherit_id" ref="stock.view_category_property_form"/>
            <field name="arch" type="xml">
            	<field name="property_stock_valuation_account_id" position="after">
            		<field name="prop_use_value_act_as_invoice"/>
            	</field>
            </field>
        </record>        
