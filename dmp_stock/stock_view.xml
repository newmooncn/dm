            	<xpath expr="/tree" position="replace">
                <tree string="Stock Inventory Lines" editable="bottom">
                	<field name="inventory_id" readonly="1"/>
                    <field name="product_id" readonly="1"/>
                    <field name="product_qty"  attrs="{'readonly': [('state','!=','draft')]}"/>
