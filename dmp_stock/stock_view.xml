            <field name="inherit_id" ref="stock.view_picking_out_search"/>
            <field name="arch" type="xml">
                <field name="stock_journal_id" position="before">
                	<field name="date"/>
                	<field name="date_done" string="Date of Deliver"/>
                	<field name="partner_id"/>
                </field>
            </field>
        </record>
        <record id="view_picking_in_search" model="ir.ui.view">
            <field name="name">dmp.stock.picking.in.search</field>
            <field name="model">stock.picking.in</field>
            <field name="inherit_id" ref="stock.view_picking_in_search"/>
            <field name="arch" type="xml">
            	<field name="name" position="after">
            		<field name='deliver_ticket_no'/>
            	</field>	
                <field name="stock_journal_id" position="before">
                	<field name="date"/>
                	<field name="date_done" string="Date of Reception"/>
                	<field name="partner_id"/>
                </field>
            </field>
        </record>

<!-- stock physical inventory -->        

        <record id="dmp_view_inventory_form" model="ir.ui.view">
            <field name="name">dmp.stock.inventory.form</field>
            <field name="model">stock.inventory</field>
            <field name="inherit_id" ref="stock.view_inventory_form"/>
            <field name="arch" type="xml">
            	<xpath expr="//button[@string='Fill Inventory']" position="attributes">
            		<attribute name="states">draft</attribute>           	
            	</xpath>
            	<xpath expr="//button[@string='Fill Inventory']" position="after">
					<button name="%(action_view_stock_import_inventory)d" string="Import Inventory" states="draft" type="action" />           	
            	</xpath>
            	<xpath expr="//page[@string='Posted Inventory']//field[@name='state']" position="attributes">
            		<attribute name="invisible">False</attribute>            	
            	</xpath>
            	<field name="move_ids" position="attributes">
            		<attribute name="readonly">True</attribute>
            	</field>
            	<xpath expr="//page[@string='General Information']//tree/field[@name='product_id']" position="after">
            		<!-- <field name="image_medium" widget="image"/> -->            	
            	</xpath>
            	<field name="date" position="after">
            		<field name="create_uid"/>
            	</field>
            	<field name="name" position="after">
					<field name="comments" placeholder="Comments..."/>   
            	</field>
<!--             	<xpath expr="//sheet/group" position="after">
                	<label for="comments"/>         	
            	</xpath> -->
            </field>
        </record>
         <record id="dmp_view_inventory_filter" model="ir.ui.view">
            <field name="name">dmp.stock.inventory.filter</field>
            <field name="model">stock.inventory</field>
            <field name="inherit_id" ref="stock.view_inventory_filter"/>
            <field name="arch" type="xml">
                <field name="company_id" position="after">
